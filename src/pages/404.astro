<style>
    body{
        background: #0b253c;
    }
 .cloud {
  background: #d8e9f6;
  border-radius: 100px;
  height: 120px;
  margin: 190px auto 20px;
  position: relative;
  width: 350px;
  z-index: 3;
}
.cloud:after, .cloud:before {
  background: #d8e9f6;
  content: "";
  position: absolute;
  z-index: -1;
}
.cloud:after {
  border-radius: 100px;
  height: 100px;
  left: 50px;
  top: -50px;
  width: 100px;
}
.cloud:before {
  height: 180px;
  border-radius: 200px;
  right: 50px;
  top: -90px;
  width: 180px;
}

.cloud_eye {
  background: #fff;
  border-radius: 100%;
  bottom: 90px;
  display: block;
  height: 40px;
  position: absolute;
  width: 30px;
}
.cloud_eye:before {
  background: #000;
  bottom: 5px;
  border-radius: 100%;
  content: "";
  height: 15px;
  right: 5px;
  opacity: 0.4;
  position: absolute;
  width: 10px;
}
.cloud_eye:after {
  border-radius: 60%;
  border-top: 6px solid #b4bec5;
  content: "";
  display: block;
  height: 30px;
  left: -2px;
  position: absolute;
  top: -10px;
  transform: rotate(-20deg);
  width: 30px;
}
.cloud_eye--right {
  right: 100px;
}
.cloud_eye--left {
  right: 150px;
}

.cloud_mouth {
  background: #b4bec5;
  border-radius: 100%;
  bottom: 30px;
  display: block;
  height: 30px;
  position: absolute;
  right: 100px;
  width: 20px;
}

.cloud_cheek {
  background: red;
  border-radius: 100%;
  bottom: 60px;
  display: block;
  height: 30px;
  opacity: 0.06;
  position: absolute;
  width: 30px;
}
.cloud_cheek--right {
  right: 70px;
}
.cloud_cheek--left {
  right: 170px;
}

.cloud_arm {
  border: solid 3px #b4bec5;
  border-radius: 100%;
  display: block;
  position: absolute;
  width: 100px;
}
.cloud_arm--left {
  border-color: transparent transparent transparent #b4bec5;
  bottom: 50px;
  height: 180px;
  left: 30px;
}
.cloud_arm--right {
  border-color: transparent #b4bec5 transparent transparent;
  bottom: 84px;
  height: 120px;
  right: 0px;
}

.cloud_drop:after, .cloud_drop:before {
  background: #e2eef7;
  border-radius: 100%;
  bottom: -10px;
  content: "";
  display: block;
  height: 20px;
  position: absolute;
  transform: scale(0.7, 1);
  width: 10px;
}
.cloud_drop--left:before {
  animation: rain 1s infinite;
  left: 100px;
}
.cloud_drop--left:after {
  animation: rain 1100ms infinite;
  animation-delay: 100ms;
  left: 150px;
}
.cloud_drop--right:before {
  animation: rain 900ms infinite;
  right: 100px;
}
.cloud_drop--right:after {
  animation: rain 1300ms infinite;
  animation-delay: 120ms;
  right: 150px;
}

@keyframes rain {
  0% {
    bottom: -10px;
    opacity: 1;
  }
  100% {
    bottom: -290px;
    opacity: 0;
  }
}
.textContainer{
    text-align: center;
    font-family: "Montserrat",sans-serif;
    color:#fff;
    margin-top: 100px;
    font-size: 22px;
    letter-spacing: 1px;
    .titleText{
        font-size: 84px;
        letter-spacing: 4px;
        margin-bottom: 30px;
    }
}
.returnButton{
    border:none;
    display: block;
    padding: 10px 20px;
    max-width: 200px;
    border-radius: 10px;
    margin: 40px auto 0 auto;
    border 0;
    background: #004F6A;
    font: 22px "Montserrat",sans-serif;
    color: #fff;
    text-decoration: none;
}
.returnButton:hover{
    background: transparent;
    border: 2px solid #fff;
    color: #fff;
}
</style>